#include <bits/stdc++.h>
using namespace std;

int main() {

vector<int> v;
v.push_back(1);
v.push_back(2);
v.push_back(3);
v.push_back(4);
v.push_back(5);

sort(v.begin(),v.end()); // sort in increasing order
for(int i =0;i<v.size();i++){        // 1 2 3 4 5 
    cout<<v[i]<<" ";
}

cout<<"\n";

sort(v.rbegin(),v.rend());// sort in decreasing order 
for(int i =0;i<v.size();i++){   // 5 4 3 2 1 
    cout<<v[i]<<" ";
}

}
