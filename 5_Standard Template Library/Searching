#include<bits/stdc++.h>
using namespace std;

int main() {

vector<int> v;
v.push_back(7);
v.push_back(8);
v.push_back(9);
v.push_back(10);
v.push_back(11);  // 7 8 9 10 11 
         //   index  0 1 2  3  4
         // iterator 1 2 3  4  5 
cout<<binary_search(v.begin(),v.end(),9)<<"\n";// Print 1   // search number if present in vector give 1 ,if not give 0
 cout<<find(v.begin(),v.end(),9)- v.begin()<<"\n";  // print  2  // 3 -1 = 2    // Give index 


}
