#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    
vector<int> v(n);
for(int i =0;i<n;i++){
    cin>>v[i];
}

for(int i =0;i<v.size();i++){     // print vector all element
cout<<v[i]<<" "; 
}
cout<<"\n";
v[0] =8;                         // Update Element value in vector 

for(int i =0;i<v.size();i++){    // print updated vector 
    cout<<v[i]<<" "; 
}

cout<<"\n";
cout<<"Size of vector   "<<v.size()<<"\n";
cout<<"capacity of vector   "<<v.capacity()<<"\n";
cout<<"\n";

v.push_back(4);     // add new element (capacity double of current size of vector  )

for(int i =0;i<v.size();i++){    //  Print All Element After add new elemnet 
    cout<<v[i]<<" "; 
}
cout<<"\n";
cout<<"Size of vector   "<<v.size()<<"\n";
cout<<"capacity of vector   "<<v.capacity()<<"\n";

}


