#include <bits/stdc++.h>
using namespace std;

int main() {
    
   vector<int> v; 
    v.push_back(1);
    v.push_back(2);
    v.push_back(3);
    v.push_back(4);
    v.push_back(5);
cout<<v.size()<<"\n";
cout<<v.capacity();    
    
v.erase(v.begin()+1);        // Delete 2 at index 1
cout<<"\n";
for(int i =0;i<v.size();i++){     
cout<<v[i]<<" "; 
}
cout<<"\n";
v.insert(v.begin()+1,70);    // insert value at index 1 
for(int i =0;i<v.size();i++){     
cout<<v[i]<<" "; 
}
cout<<"\n";
v[1] = 77;              // update value at index 1 

for(int i =0;i<v.size();i++){     
cout<<v[i]<<" "; 
}



// Your Output
// 5
// 8
// 1 3 4 5 
// 1 70 3 4 5 
// 1 77 3 4 5
