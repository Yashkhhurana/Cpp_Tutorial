#include <bits/stdc++.h>
using namespace std;

int main() {
    
   vector<int> v; 
    v.push_back(1);
    v.push_back(2);
    v.push_back(3);
    v.push_back(4);
    v.push_back(5);
v.pop_back();          // last element 
cout<<v.size()<<"\n";
cout<<v.capacity();    
    
v.erase(v.begin()+1);// Delete 2 at index 1    // any vector value you delete by just adding 
cout<<"\n";
for(int i =0;i<v.size();i++){     
cout<<v[i]<<" "; 
}
}
